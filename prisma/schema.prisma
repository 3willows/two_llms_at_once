generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Post {
  id   String @id @default(cuid())
  name String
}

model Prompt {
  id   String @id @default(cuid())
  content String
}

model Google {
  id   String @id @default(cuid())
  content String
}
